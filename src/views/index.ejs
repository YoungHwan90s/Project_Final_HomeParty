        %><%-include('user-menu-edit.ejs') %><% } %> <% if (components === 'wish-list') {
        %><%-include('user-menu-wish-list.ejs') %><% } %> <% if (components === 'manage') {
        %><%-include('manage.ejs') %><% } %> <% if (components === 'manage-host') {
        %><%-include('manage-host.ejs') %><% } %> <% if (components === 'manage-guest') {
        %><%-include('manage-guest.ejs') %><% } %> <% if (components === 'manage-history') {
        %><%-include('manage-history.ejs') %><% } %> <% if (components === 'admin-user') {
        %><%-include('admin-user.ejs') %><% } %> <% if (components === 'admin-party') {
